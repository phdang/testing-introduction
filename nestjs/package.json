  "scripts": {
    "eslint": "eslint src/",
    "build": "npm run migrate:deploy && npm run eslint && nest build",
    "build:dev": "npm run migrate:dev && npm run eslint && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:format": "prisma format",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts",
    "start": "npm run migrate:dev && npm run eslint && nest start",
    "start:dev": "npm run eslint && nest start --watch",
    "start:debug": "npm run eslint && nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "rimraf .husky&&husky install&&husky add .husky/pre-commit \"npm run precommit\"",
    "precommit": "lint-staged"
  },
